<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="500" prefWidth="800" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.StatControllervehicule">
    <children>
        <BarChart fx:id="barChart" layoutX="30.0" layoutY="20.0" prefHeight="460.0" prefWidth="760.0" >
            <xAxis>
                <CategoryAxis label="categorie" />
            </xAxis>
            <yAxis>
                <NumberAxis label="vehicule par categorie" prefHeight="394.0" prefWidth="67.0" />
            </yAxis>
        </BarChart>
      <Button fx:id="btnretour" layoutX="14.0" layoutY="455.0" mnemonicParsing="false" onAction="#btnretour" text="retour" />
    </children>
</AnchorPane>
